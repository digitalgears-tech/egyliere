<apex:page renderAs="pdf"
  applyBodyTag="false"
  standardController="Opportunity"
  extensions="PricingQuotationExtension"
>
  <head>
    <style type="text/css" media="print">
      @page {
        padding-top: 40px;
        @top-center {
          content: element(header);
        }
        @bottom-left {
          content: element(footer);
        }
      }

      div.header {
        padding-top: 30px;
        padding-right: 10px;
        padding-left: 10px;

        position: running(header);
      }

      div.footer {
        display: block;
        padding: 5px;
        position: running(footer);
      }

      .pagenumber:before {
        content: counter(page);
      }

      .pagecount:before {
        content: counter(pages);
      }
      .header-text {
        font-size: 1.3em;
        font-family: Arial, Helvetica, sans-serif;
        font-weight: 700;
      }
      .subheader-text {
        font-weight: 20;
        font-size: 12px;
        font-weight: lighter;
        font-family: Arial, Helvetica, sans-serif;
      }
      .table-header-text {
        font-weight: 20;
        font-size: 12px;
        font-weight: bold;
        font-family: Arial, Helvetica, sans-serif;
        color: white;
      }
      ul {
        list-style-type: none;
        margin: 0;
        padding: 0;
        padding-top: 5px;
      }
      span.mark {
        content: "\2713";
      }

      td.border,
      th.border {
        border: 1px solid #36495f;
      }

      table {
        border-collapse: collapse;
        width: 100%;
      }
      table.break {
        page-break-inside: auto;
      }
      tr {
        page-break-inside: avoid;
        page-break-after: auto;
        border: 1px solid #36495f;
      }

      th {
        background-color: #36495f;
        text-align: center;
      }
      td {
        word-break: break-all;
        word-wrap: break-word;
      }
      td.center {
        text-align: center;
      }
    </style>
